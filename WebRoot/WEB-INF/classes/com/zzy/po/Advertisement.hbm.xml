<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.zzy.po">
	<class name="Advertisement" table="advertisement">
		<id name="id">
			<generator class="native"/>
		</id>
		<property name="describe" type="java.lang.String" length="50" not-null="true"/>
		<property name="path" type="java.lang.String" not-null="true"/>
		<property name="position" type="integer" not-null="true"/>
		<property name="show" type="true_false"/>
		<many-to-one name="adimg" class="AdImg" column="adimg_id" unique="true" fetch="join"/>
	</class>
</hibernate-mapping>
